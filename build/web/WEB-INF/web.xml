<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Mytasksmanager</servlet-name>
        <servlet-class>com.mounia.tasks.MytasksmanagerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Serve</servlet-name>
        <servlet-class>com.mounia.tasks.Serve</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>com.mounia.tasks.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>External</servlet-name>
        <servlet-class>com.mounia.tasks.External</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Mytasksmanager</servlet-name>
        <url-pattern>/mytasksmanager</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>JsontaskServlet</servlet-name>
        <servlet-class>com.mounia.tasks.JsonTaskServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cron</servlet-name>
        <servlet-class>com.mounia.tasks.Cron</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Feed</servlet-name>
        <servlet-class>com.mounia.tasks.Feed</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JsontaskServlet</servlet-name>
        <url-pattern>/jsontaskmanager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Serve</servlet-name>
        <url-pattern>/serve</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>External</servlet-name>
        <url-pattern>/external</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cron</servlet-name>
        <url-pattern>/cron</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Feed</servlet-name>
        <url-pattern>/feed</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
